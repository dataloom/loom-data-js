{
  "name": "loom-data",
  "version": "0.12.2",
  "homepage": "https://github.com/kryptnostic/loom-data-js",
  "description": "JavaScript SDK for all Loom REST APIs",
  "author": {
    "name": "Kryptnostic, Inc.",
    "email": "info@kryptnostic.com",
    "url": "https://www.kryptnostic.com"
  },
  "contributors": [
    {
      "name": "Hristo Oskov",
      "email": "hristo@kryptnostic.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kryptnostic/loom-data-js.git"
  },
  "bugs": {
    "url": "https://github.com/kryptnostic/loom-data-js/issues"
  },
  "license": "Apache-2.0",
  "main": "build/loom-data.node.js",
  "browser": "build/loom-data.js",
  "dependencies": {
    "axios": "^0.15.0",
    "immutable": "^3.8.1",
    "lodash": "^4.16.4",
    "loglevel": "^1.4.1",
    "moment": "^2.15.1",
    "uuid-validate": "0.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "better-npm-run": "0.0.14",
    "bluebird": "^3.4.6",
    "documentation": "^4.0.0-beta",
    "eslint": "^3.13.0",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-flowtype": "^2.29.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jasmine": "^2.2.0",
    "flow-bin": "^0.37.4",
    "jasmine": "^2.5.2",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-diff-reporter": "^0.6.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.0",
    "npm-run-all": "^4.0.0",
    "phantomjs-prebuilt": "^2.1.13",
    "rimraf": "2.5.4",
    "webpack": "^2.2.0-rc",
    "webpack-dev-server": "^2.2.0-rc"
  },
  "scripts": {
    "build": "npm-run-all --parallel build:prod build:prod:node",
    "build:min": "npm-run-all --parallel build:prod:min build:prod:node:min",
    "build:prod": "better-npm-run build:prod",
    "build:prod:min": "better-npm-run build:prod:min",
    "build:prod:node": "better-npm-run build:prod:node",
    "build:prod:node:min": "better-npm-run build:prod:node:min",
    "clean": "rimraf build/*",
    "flow": "flow check",
    "lint": "eslint --config .eslintrc src test",
    "release:major": "npm version major -m \"releasing v%s\"",
    "release:minor": "npm version minor -m \"releasing v%s\"",
    "release:patch": "npm version patch -m \"releasing v%s\"",
    "version": "npm-run-all clean lint --parallel build build:min && git add -A package.json build/",
    "postversion": "git push && git push --tags",
    "test": "better-npm-run test",
    "test:karma:run": "better-npm-run test:karma:run",
    "test:karma:start": "better-npm-run test:karma:start",
    "test:karma:watch": "better-npm-run test:karma:watch",
    "test:karma:browser": "better-npm-run test:karma:browser",
    "webpack-dev-server": "better-npm-run webpack-dev-server"
  },
  "betterScripts": {
    "build:prod": {
      "command": "webpack --config config/webpack/webpack.config.babel.js",
      "env": {
        "BUILD": "production",
        "MINIMIZE": "false",
        "TARGET_ENV": "web"
      }
    },
    "build:prod:min": {
      "command": "webpack --config config/webpack/webpack.config.babel.js",
      "env": {
        "BUILD": "production",
        "MINIMIZE": "true",
        "TARGET_ENV": "web"
      }
    },
    "build:prod:node": {
      "command": "webpack --config config/webpack/webpack.config.babel.js",
      "env": {
        "BUILD": "production",
        "MINIMIZE": "false",
        "TARGET_ENV": "node"
      }
    },
    "build:prod:node:min": {
      "command": "webpack --config config/webpack/webpack.config.babel.js",
      "env": {
        "BUILD": "production",
        "MINIMIZE": "true",
        "TARGET_ENV": "node"
      }
    },
    "test": {
      "command": "karma start config/karma/karma.config.js",
      "env": {
        "BUILD": "test"
      }
    },
    "test:karma:run": {
      "command": "karma run config/karma/karma.config.js --",
      "env": {
        "BUILD": "development"
      }
    },
    "test:karma:start": {
      "command": "karma start config/karma/karma.config.js --singleRun=false",
      "env": {
        "BUILD": "development"
      }
    },
    "test:karma:watch": {
      "command": "karma start config/karma/karma.config.js --singleRun=false --autoWatch=true",
      "env": {
        "BUILD": "development"
      }
    },
    "test:karma:browser": {
      "command": "karma start config/karma/karma.config.js --singleRun=false --autoWatch=true --browsers=Chrome --reporters=kjhtml",
      "env": {
        "BUILD": "development"
      }
    },
    "webpack-dev-server": {
      "command": "webpack-dev-server --config config/webpack/webpack.config.babel.js --port 9000 --content-base build/",
      "env": {
        "BUILD": "development"
      }
    }
  }
}
