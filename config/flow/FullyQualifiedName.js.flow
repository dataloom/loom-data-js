/*
 * @flow
 */

declare type FQNObject = {|
  namespace :string;
  name :string;
|};

declare class FullyQualifiedName {
  static isValid :(...args :any[]) => boolean;
  static toString :(...args :any[]) => string;
  namespace :string;
  name :string;
  getNamespace() :string;
  getName() :string;
  toObject() :FQNObject;
  toString() :string;
  valueOf() :string;
}

type FQN = FullyQualifiedName;

export type {
  FQN,
  FullyQualifiedName
};
